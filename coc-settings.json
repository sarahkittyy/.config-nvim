{
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": [
        "--lsp"
      ],
      "rootPatterns": [
        ".stack.yaml",
        ".hie-bios",
        "BUILD.bazel",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": true,
          "maxNumberOfProblems": 10,
          "completionSnippetsOn": true
        }
      }
    }
  },
  "clangd.arguments": ["--fallback-style=none"],
  "suggest.noselect": true,
  "suggest.floatConfig": {
    "border": true,
    "shadow": false,
    "rounded": true
  },
  "diagnostic.floatConfig": {
    "border": true,
    "shadow": false,
    "winblend": true,
    "highlight": "Normal",
    "rounded": true
  },
  "signature.floatConfig": {
    "border": true,
    "shadow": false,
    "rounded": true
  },
  "hover.floatConfig": {
    "border": true,
    "shadow": false,
    "rounded": true
  },
  "coc.preferences.semanticTokensHighlights": false,
  "clangd.compilationDatabasePath": "build/",
  "java.errors.incompleteClasspath.severity": "ignore",
  "typescript.format.enabled": false,
  "typescript.showUnused": false,
  "javascript.showUnused": false,
  "javascript.format.enabled": false,
  "javascript.validate.enable": false,
  "suggest.minTriggerInputLength": 2,
  "eslint.autoFixOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript", "typescriptreact", "javascriptreact"],
  "prettier.requireConfig": true,
  "eslint.validate": ["javascript", "typescript"],
  "eslint.lintTask.options": [".", "--ext", ".ts"],
  "json.enable": false,
  "suggest.completionItemKindLabels": {
    "method": "  method",
    "function": "  fn",
    "variable": "[] var",
    "field": "  field",
    "typeParameter": "<> type",
    "constant": "  constant",
    "class": " פּ class",
    "interface": " 蘒interface",
    "struct": "  struct",
    "event": "  event",
    "operator": "  operator",
    "module": "  module",
    "property": "  prop",
    "enum": " 練enum",
    "reference": "  ref",
    "keyword": "  keyword",
    "file": "  file",
    "folder": " ﱮ folder",
    "color": "  color",
    "unit": " 塞 unit",
    "snippet": "  snippet",
    "text": "  text",
    "constructor": "  constructor",
    "value": "  value",
    "enumMember": "  enum"
  },
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/15.0.3/clangd_15.0.3/bin/clangd"
}
